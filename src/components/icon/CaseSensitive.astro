---
interface Props {
  size?: number;
  class?: string;
}

const {
  size = 24,
  class: customClass = '',
} = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  width={size}
  height={size}
  viewBox="0 0 24 24"
  fill="none"
  stroke="currentColor"
  stroke-width="2"
  stroke-linecap="round"
  stroke-linejoin="round"
  class={`lucide lucide-case-sensitive-icon lucide-case-sensitive ${customClass}`}
>
  <path d="m3 15 4-8 4 8" />
  <path d="M4 13h6" />
  <circle cx="18" cy="12" r="3" />
  <path d="M21 9v6" />
</svg>
